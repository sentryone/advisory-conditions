\\r\\nJOIN _UsedMemory AS um ON tm.object_name = um.object_name\\r\\n\\tAND tm.instance_name = um.instance_name\\r\\nWHERE tm.instance_name IN\\r\\n\\t(\\r\\n\\t\\tSELECT rgp.name\\r\\n\\t\\tFROM sys.resource_governor_resource_pools AS rgp\\r\\n\\t\\tJOIN sys.databases AS db ON rgp.pool_id = db.resource_pool_id\\r\\n\\t)\\r\\n;\",\"InstanceType\":1},\"ComparisonType\":3,\"Right\":{\"ValueTypeID\":\"07f87b7f-c063-47a2-a5be-772ba85ed827\",\"Value\":\"80\"},\"ID\":\"1\"}],\"BooleanOperationType\":0,\"ID\":\"0\"}","PublisherID":0,"PublishDateUtc":"2017-05-24T19:28:56","Rights":0,"SignatureVersion":1,"SignaturePublicKey":"<RSAKeyValue><Modulus>uzJQ9gzevXFwOgw/hkcAtD+cA/bBbD1PRzhEZCxbZ6YwjJ1c9bbfXFItLQNwnm8bdWh2k57//qbpEj5DFHOW2EAjHc2Zw5m/vACm6OzelubPS5hbWvzshlaBJKm7KrpWQpPZClx/5eVvUVzOtlz+44RRTiOszObT58acJAQwA70=</Modulus><Exponent>AQAB</Exponent></RSAKeyValue>","Signature":"gOrewz1J1D9oVe9cO1mZFfZ/nyjq4/eSajQ36qXYxXmPSXHAmffnZz0tE/Tgd35yPQR085LpaXvyYJFSDSuD6g47isBQhZlpLVJMYY6Y8kux7/gL+zxce1jLXRBxy/pyyJP6l1CJOvcN/Gipx4O42unL5waHMfCXdKBtHq5U+44=","IsSelfPublishedCondition":true},"Tags":"Needed?","Areas":["SQL Server: Memory Usage"],"ConditionSystemVersion":3,"MinDBSchemaVersion":null,"MaxDBSchemaVersion":null,"Items":[]}